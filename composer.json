{
  "name": "elgigi/flysystem-useful-adapters",
  "description": "Useful adapters for Flysystem PHP library",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "ElGigi\\FlysystemUsefulAdapters\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ElGigi\\FlysystemUsefulAdapters\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "league/flysystem": "^3.7",
    "league/flysystem-read-only": "^3.23"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0 || ^10.0",
    "psr/log": "^1.0 || ^2.0 || ^3.0",
    "league/flysystem-adapter-test-utilities": "^3.21",
    "league/flysystem-memory": "^3.24"
  }
}
